#  The version for use with C and INGRES follows:

include ../../Environ.10
include make_include64

######################### Directives ###############################

TARGET = $(HDB_APP_BIN)/site_data
OBJS = \
		$(OBJDIR)/site_data.o

main: $(TARGET)

clean:
	rm -f $(TARGET)  $(OBJS)

$(TARGET): $(OBJS)
	$(CC) $(OBJS) $(LIBS) -o $(TARGET)

$(OBJDIR)/site_data.o: \
					site_data.pc 
	proc iname=site_data.pc include=$(HDB_APP_INCLUDE)
	$(CC) $(CFLAGS) -c site_data.c -o $@
