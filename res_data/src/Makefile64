#  The version for use with C and INGRES follows:

include ../../Environ.10
include make_include64

######################### Directives ###############################

TARGET = $(HDB_APP_BIN)/res_data
OBJS = \
		$(OBJDIR)/res_data.o

main: $(TARGET)

clean: 
	rm -f  $(TARGET)  $(OBJS)

$(TARGET): $(OBJS)
	$(CC) $(OBJS) $(LIBS) -o $(TARGET)

$(OBJDIR)/res_data.o: \
					res_data.pc 
	proc iname=res_data.pc include=$(HDB_APP_INCLUDE)
	$(CC) $(CFLAGS) -c res_data.c -o $@
